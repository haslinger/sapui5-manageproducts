<core:FragmentDefinition
		xmlns="sap.m"
		xmlns:l="sap.ui.layout"
		xmlns:core="sap.ui.core">
	<ResponsivePopover id="_IDGenResponsivePopover1" title="{i18n>dimensionsTitle}"
		class="sapUiPopupWithPadding">
		<content>
			<l:Grid id="_IDGenGrid1">
				<l:content>
					<Text id="_IDGenText1" text="{i18n>productWidth}: {
							path: 'Width', 
							formatter: '.formatter.numberUnit'
						} {DimUnit}" />
					<Text id="_IDGenText2" text="{i18n>productDepth}: {
							path: 'Depth',
							formatter: '.formatter.numberUnit'
						} {DimUnit}" />
					<Text id="_IDGenText3" text="{i18n>productHeight}: {
							path: 'Height',
							formatter: '.formatter.numberUnit'
						} {DimUnit}" />
					<Text id="_IDGenText4" text="{i18n>productWeight}: {
							path: 'WeightMeasure',
							formatter: '.formatter.numberUnit'
						} {WeightUnit}" />
				</l:content>
			</l:Grid>
		</content>
	</ResponsivePopover>
</core:FragmentDefinition>